{
    "env": {
        "es2021": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [],
    "rules": {
        "quotes": [
            "error",
            "double"
        ],
        "no-useless-return": "warn",
        "no-dupe-args": "warn",
        "no-irregular-whitespace": "warn",
        "no-unreachable": "warn",
        "default-case": "warn",
        "no-empty-function": "warn",
        "array-bracket-newline": [
            "error",
            {
                "multiline": true
            }
        ],
        "brace-style": [
            "error",
            "allman"
        ],
        "capitalized-comments": "warn",
        "comma-dangle": [
            "error",
            "never"
        ],
        "comma-spacing": [
            "error",
            {
                "before": false,
                "after": true
            }
        ],
        "indent": [
            "error",
            4
        ]
    }
}