commands:
    01_python_install:
        test: '[ ! -f /usr/bin/python3.7 ] && echo "Python 3.7 not installed"'
        command: 'yum install -y python3.7'

    02_pip_install:
        cwd: /var/app/current
        command: 'python3 -m pip install --upgrade pip'
        command: 'pip install -r requirements.txt'